{
  "env": {
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "curly": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "indent": [2, 2, {"SwitchCase": 0}],
    "no-fallthrough": 2,
    "no-else-return": 2,
    "no-eq-null": 2,
    "no-redeclare": 2,
    "no-undef": 2,
    "no-unused-vars": 2,
    "quotes": [2, "single"],
    "radix": 2,
    "semi": [2, "always"],
    "strict": [2, "global"]
  }
}
